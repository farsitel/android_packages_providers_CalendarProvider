<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title for the Calendar Storage process. -->
    <string name="calendar_storage">حافظهٔ تقویم</string>

    <!-- Title for calendar gadget when displayed in list of all other gadgets -->
    <string name="gadget_title">تقویم</string>

    <!-- Title of event when no explicit title is specified by the user -->
    <string name="no_title_label">(بدون عنوان)</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <string name="calendar_default_name">پیش‌فرض</string>

    <!-- Shown in gadget when additional events are available for display, but no room remaining -->
    <plurals name="gadget_more_events">
        <!-- additional events message for 1 event -->
        <item quantity="one">یک رویداد دیگر</item>
        <!-- additional events message for multiple events -->
        <item quantity="other"><xliff:g id="number">%Ld</xliff:g> رویداد دیگر</item>
    </plurals>

    <!-- Caption to show on gadget when there are no upcoming calendar events -->
    <string name="gadget_no_events">فعلاً رویدادی ندارید.</string>

    <!-- Caption on secret calendar info -->
    <string name="calendar_info">اطلاعات تقویم</string>

    <!-- Error message for secret calendar info -->
    <string name="calendar_info_error">خطا</string>

    <!-- No calendars message for secret calendar info -->
    <string name="calendar_info_no_calendars">تقویمی وجود ندارد.</string>

    <!-- Event count message for secret calendar info -->
    <string name="calendar_info_events">رویدادها: <xliff:g id = "events">%1$Ld</xliff:g></string>

    <!-- Event and dirty event count message for secret calendar info -->
    <string name="calendar_info_events_dirty">رویدادها: <xliff:g id = "events">%1$Ld</xliff:g>، ذخیره‌نشده: <xliff:g id = "dirty events">%2$Ld</xliff:g></string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">تقویم</string>

    <!-- Format of the day of week, the header of the gadget. %a for Sun, %A for Sunday -->
    <string name="gadget_day_of_week_format">%A</string>

</resources>
